<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

	<Script file="Footer.lua"/>


    <Frame name="MUHAPFooterTemplate" mixin="MUHAPFooterMixin"  virtual="true">
		<Frames>

			<Frame parentKey="TextAuctions" inherits="MUHAPEntryTextTemplate">
                <KeyValues>
                    <KeyValue key="type" value="MUHAP_TEXT_TYPE.Auctions" type="global"/>
                </KeyValues>
                <Anchors>
                    <Anchor point="RIGHT" relativeKey="$parent" relativePoint="RIGHT" x="-170" y="-5" />
                </Anchors>
            </Frame>

			<Frame parentKey="TextCheck" inherits="MUHAPEntryTextTemplate">
                <KeyValues>
                    <KeyValue key="type" value="MUHAP_TEXT_TYPE.Check" type="global"/>
                </KeyValues>
                <Anchors>
                    <Anchor point="RIGHT" relativeKey="$parent" relativePoint="RIGHT" x="-170" y="5" />
                </Anchors>
            </Frame>



			<Button parentKey="PostButton"  mixin="MUHAPFooterPostButtonMixin"  frameLevel="320"  inherits="UIPanelButtonTemplate" text="Post Auctions"  hidden="true">
				<Size x="158" y="22"/>
				<Anchors>
					<Anchor point="TOPRIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnClick method="OnClick"/>
				</Scripts>
			</Button>
			<Button parentKey="CheckButton"  mixin="MUHAPFooterCheckButtonMixin"  frameLevel="300"  inherits="UIPanelButtonTemplate" text="Run Check" >
				<Size x="158" y="22"/>
				<Anchors>
					<Anchor point="TOPRIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnClick method="OnClick"/>
				</Scripts>
			</Button>
		</Frames>
		<Anchors>
			<Anchor point="TOPRIGHT" relativeKey="$parent.ScrollFrame" relativePoint="BOTTOMRIGHT" x="25" y="0"/>
			<Anchor point="BOTTOMLEFT" relativeKey="$parent.ScrollFrame" relativePoint="BOTTOMLEFT" x="0" y="-24"/>
		</Anchors>
		
	</Frame>

</Ui>